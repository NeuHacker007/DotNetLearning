@model IEnumerable<EmployeeManagementWeb.Models.Employee>
@{ 
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Index";
}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.ID</td>
                <td>@employee.EmployeeName</td>
                <td>@employee.Department</td>
            </tr>
        }
    </tbody>
</table>


