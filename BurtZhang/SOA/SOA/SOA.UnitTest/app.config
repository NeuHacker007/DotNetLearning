<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="MyWebServiceSoap" />
                <binding name="BasicHttpBinding_IMyWCF" />
                <binding name="BasicHttpBinding_IMathService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/Remoting/MyWebService.asmx"
                binding="basicHttpBinding" bindingConfiguration="MyWebServiceSoap"
                contract="SOAUnitTest.MyWebServiceSoap" name="MyWebServiceSoap" />
            <endpoint address="http://localhost:8080/WCF/MyWCF.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMyWCF" contract="MyWebWCF.IMyWCF"
                name="BasicHttpBinding_IMyWCF" />
            <endpoint address="http://localhost:8080/MathService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMathService" contract="ConsoleWCF.IMathService"
                name="BasicHttpBinding_IMathService" />
        </client>
    </system.serviceModel>
</configuration>